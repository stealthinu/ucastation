// WebSocketでの通信部分
var port = '<%= port %>';

var socket = io.connect( "/view", { port: port } );

socket.on( 'connect', function() {
//  console.log( "connect." );
});

socket.on( 'channel', function( channel ) {
//  console.log( "receive channel: " + channel );
  Player.play( channel, 'ust_main' );
});

socket.on( 'board', function( text ) {
//  console.log( "receive board: " + text );
  $("#admin_board").html( text );
});
