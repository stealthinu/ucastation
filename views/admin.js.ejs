// WebSocketでの通信部分
var port = '<%= port %>';
var password = 'pass';

var socket = io.connect( null, { port: port } );

socket.on( 'connect', function() {
  console.log( "connect." );
});

socket.emit( 'admin login', password );

function sendChannel( channel ) {
  socket.emit( 'admin channel', channel );
}
