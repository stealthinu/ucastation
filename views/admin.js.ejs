// WebSocketでの通信部分
var port = '<%= port %>';
var password = 'pass';
var socket = new io.Socket( null, { port: port } );
socket.connect();

socket.on( 'connect', function() {
  console.log( "connect." );
});

socket.send( 'admin ' + password );

function sendChannel( channel ) {
  socket.send( channel );
}
